{
  "name": "npm-scripts-runner",
  "displayName": "NPM Scripts Runner",
  "description": "Run npm scripts from package.json with a click of a button",
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/xxx87/npm-scripts-runner.git"
  },
  "homepage": "https://github.com/xxx87/npm-scripts-runner/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/xxx87/npm-scripts-runner/issues"
  },
  "author": {
    "name": "xxx87"
  },
  "publisher": "xxx87",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#eff1f3",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "npm",
    "scripts",
    "runner",
    "package.json",
    "terminal"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "npm-scripts-runner.showScripts",
        "title": "Show NPM Scripts Runner"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "vsce package -o extensions/"
  },
  "dependencies": {
    "glob": "^7.2.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.17",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^9.26.0",
    "mocha": "^11.2.2",
    "typescript": "^5.8.3",
    "vscode-test": "^1.6.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
